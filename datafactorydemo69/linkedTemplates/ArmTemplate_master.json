{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactorydemo69"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2'"
		},
		"AzureSqlDatabaseLinkedService1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabaseLinkedService1'"
		},
		"SourcedLinke1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SourcedLinke1'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://firstcloudproject.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://firstcloudproject.dfs.core.windows.net/"
		},
		"AzureSqlDatabaseLinkedService1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-dbserver1.database.windows.net"
		},
		"AzureSqlDatabaseLinkedService1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "preksha"
		},
		"AzureSqlDatabaseLinkedService1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "preksha"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactorydemo69_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2_accountKey')]"
					},
					"AzureSqlDatabaseLinkedService1_password": {
						"value": "[parameters('AzureSqlDatabaseLinkedService1_password')]"
					},
					"SourcedLinke1_connectionString": {
						"value": "[parameters('SourcedLinke1_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_url')]"
					},
					"AzureSqlDatabaseLinkedService1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabaseLinkedService1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabaseLinkedService1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabaseLinkedService1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabaseLinkedService1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabaseLinkedService1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}